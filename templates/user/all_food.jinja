{% extends 'base.jinja' %}  {% block content %}

{# SHOW USER'S OWN LIST #}
    <h1 class="user-list__title">{{ user.name }}</h1>
    <ul class="user-list">
        <li class="user-list__items">
            {% for food in foods %}
                {% if user.id == food.user_id %}
                    <a href="/{{ food.id }}/edit">{{ food.name}}: {{ food.catergory }}</a>
                {% endif %}
            {% endfor %}
        </li>
    </ul>

{% endblock content %}